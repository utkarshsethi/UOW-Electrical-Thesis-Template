\def\titlep{
	\thispagestyle{empty}
	\begin{center}
	\Large{\textbf{\@title}}
	\vskip13mm
	\large{A thesis submitted in partial fulfilment of the\\
	requirements for the award of the degree}
	\vskip12mm
	\large{\textbf{\@degreetitle}}
	\vskip12mm
	\large{\textbf{from}}
	\vskip12mm
	\large{\textbf{University of Wollongong}}
	\vskip12mm
	\large{\textbf{by}}
	\vskip12mm
	\textbf{\@author}
	\vskip12mm
	\textbf{School of Electrical, Computer and Telecommunications Engineering}
	\vskip12mm
	\textbf{\@submitdate}
	\vskip12mm
	Supervisor:~\@supervisor
	\end{center}
	\newpage}
\renewcommand{\baselinestretch}{1.5}
