\def\beforeabstract{
	\pagenumbering{roman}
	\pagestyle{empty}
	\titlep
  \pagestyle{plain}}

\def\tocsection#1{
	\chapter*{#1}
	\addcontentsline{toc}{chapter}{#1}}

\def\notocsection#1{
	\chapter*{#1}}

\def\afterabstract{\newpage
	\sog
	\tableofcontents
	\newpage
	\iftablespage
		{\addvspace{10pt}
		\let\saveaddvspace=\addvspace
		\def\addvspace##1{}
		\listoftables
		\let\addvspace=\saveaddvspace}
		\newpage
	\fi
	\iffigurespage
		{\addvspace{10pt}
		\let\saveaddvspace=\addvspace
		\def\addvspace##1{}
		\listoffigures
		\let\addvspace=\saveaddvspace}
		\newpage
	\fi
	\pagestyle{headings}}

